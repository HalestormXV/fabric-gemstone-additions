{
  "schemaVersion": 1,
  "id": "fabric_gemstone_additions",
  "version": "${version}",

  "name": "Gemstone Additions",
  "description": "A simple Minecraft Fabric mod that adds in some gemstones and uses for them.",
  "authors": [
    "HalestormXV"
  ],
  "contact": {
    "homepage": "https://twitch.tv/halestormxv"
  },

  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.halestormxv.fabric_gemstone_additions.FabricGemstoneAdditions"
    ]
  },
  "mixins": [
    "fabric_gemstone_additions.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.x",
    "java": ">=16"
  },
  "suggests": {
    "another-mod": "*"
  }
}
